<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net472;netstandard2.0</TargetFrameworks>
    <RootNamespace>Forksen.Qiniu</RootNamespace>
    <AssemblyName>Forksen.Qiniu</AssemblyName>
    <Version>0.1.0</Version>
    <Authors>harriszhang@live.cn</Authors>
    <Company>harriszhang@live.cn</Company>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <DocumentationFile>bin\$(Configuration)\Forksen.Qiniu.xml</DocumentationFile>
    <PackageIcon>forksen.png</PackageIcon>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/forksen/forksen-qiniu</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\forksen.png" Pack="true" PackagePath="" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.2" />
  </ItemGroup>

</Project>